{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-airbnb",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [false],
        "function-name": [false],
        "jsx-no-lambda": [false],
        "import-name": [false],
        "ordered-imports": [false],
        "no-submodule-imports": [false],
        "object-shorthand-properties-first": [false]
    },
    "rulesDirectory": []
}
